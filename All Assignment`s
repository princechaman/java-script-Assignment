CLASS ->
class Human {
     constructor(name, age ,fathername) {
    this.name = name;
    this.age = age;
    this.fathername = fathername;
  }
     FullName() {
    
    return  this.name+" s/o " + this.fathername;
  }
}
let myHuman = new Human("pappu",19, "Jangle Singh");
console.log(myHuman.FullName());


Addition
 
 <html>
    <head>
        <script>
            function Addission(){
                let number1= document.getElementById('num1').value;
                let number2 = document.getElementById('num2').value;
                let sum=parseInt(number1)+parseInt(number2);
                
                document.getElementById('sum').innerHTML=sum;
            }
        </script>
    </head>
<body>
Enter a number<input type="text" id="num1" /><br/>
Enter a number <input type="text" id="num2" /><br/>
<button onclick="Addission;">add</button>
Total : <span id="sum"></span>
</body>
</html>

